<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="address" class="cfg.lms.bkm.Student">
		<property name="studentId" value="10" />
		<property name="studentName" value="Harsh" />
		<property name="course" ref="crs" />
	</bean>

	<bean id="crs" class="cfg.lms.bkm.Course">
		<property name="cname" value="java-training" />

	</bean>

</beans>